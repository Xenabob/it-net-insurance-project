@model InsuranceCompany.Models.Orders

@{
    ViewData["Title"] = "Delete";
}

<h2>
    Delete Confirmation
</h2>

<div>
    <p>
        Are you sure you want to delete this order?
    </p>
</div>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <input class="btn btn-danger" name="confirmButton" type="submit" value="Delete" />
}